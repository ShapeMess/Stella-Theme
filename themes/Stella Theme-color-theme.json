{
	"name": "Stella Theme",
	"type": "dark",
	"colors": {
		// Editor
		"editor.background": "#252527",
		"editor.foreground": "#bebebe",
		
        "editor.lineHighlightBackground": "#ffffff0a",
        "editor.selectionBackground": "#ffffff23",

        "editorCursor.foreground": "#ecac58",
        "editor.findMatchHighlightBackground": "#ff00003d",

        "editorIndentGuide.background": "#abb2bf25",
		
        "editorLineNumber.foreground": "#4e5158",
        "editorLineNumber.activeForeground": "#998476",
		
        "editorWhitespace.foreground": "#dbe9ff28",
		
        "editorHoverWidget.background": "#242425",
        "editorHoverWidget.border": "#1b1b1b",

        "editorSuggestWidget.background": "#242425",
        "editorSuggestWidget.border": "#141414",
        "editorSuggestWidget.selectedBackground": "#2b2c2e",

        "editorWidget.background": "#242425",
        "editorWidget.border": "#141414",

		// GIT Differences
		"diffEditor.insertedTextBackground": "#8dca5b2d",
		"diffEditor.removedTextBackground": "#df272731",
		"diffEditor.border": "#bb8445",
		"diffEditor.diagonalFill": "#3a3a3d",

		// Activity Bar
        "activityBar.background": "#28282b",
        "activityBar.foreground": "#a7a9ad",
        "activityBar.border": "#141414",
		"activityBarBadge.background": "#00000000",
        "activityBarBadge.foreground": "#e4e4e4",

		// Sidebar
        "sideBar.background": "#2e2e30",
		"sideBar.foreground": "#9c9ea0",
        "sideBar.border": "#141414",
		"sideBarTitle.foreground": "#bbbbbb",
        
        "list.activeSelectionBackground": "#363638",
        "list.activeSelectionForeground": "#D7DAE0",
		
        "list.focusBackground": "#ffffff0c",
		"list.focusOutline": "#ffffff13",
        "list.hoverBackground": "#ffffff11",
        "list.highlightForeground": "#737bee",
        "list.inactiveSelectionBackground": "#363638",
        "list.inactiveSelectionForeground": "#D7DAE0",
		"list.activeSelectionIconForeground": "#D7DAE0",
		"list.inactiveSelectionIconForeground": "#D7DAE0",
		"list.dropBackground": "#ffffff15",
		
		"listFilterWidget.background": "#2e2e30",
		"listFilterWidget.outline": "#3b3b3d",
		"listFilterWidget.noMatchesOutline": "#bb5846",
		"list.filterMatchBackground": "#bb574636",
		"list.filterMatchBorder": "#bb5846",

		"list.errorForeground": "#bb5846",
		"list.warningForeground": "#bb8446",
		"list.deemphasizedForeground": "#755b57",
		
        "sideBarSectionHeader.background": "#39393d",

		"tree.indentGuidesStroke": "#ffffff3b",

		// Panels
		"panel.background": "#2e2e30",
		"panel.border": "#141414",
		"panel.dropBorder": "#bb8445",
		"panelTitle.activeBorder": "#bb8445",
		"panelTitle.activeForeground": "#abaeb9",
		"panelTitle.inactiveForeground": "#6d6e72",
		"panelSection.border": "#141414",
		"panelSection.dropBackground": "#ffffff07",
		"panelSectionHeader.background": "#39393d",
		"panelSectionHeader.foreground": "#abaeb9",
		"panelSectionHeader.border": "#141414",

		// Terminal
		"terminal.background": "#2a2a2c",
		"terminal.border": "#141414",

		"terminal.foreground": "#aaaaaa",
		
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#4261b8",
		"terminal.ansiCyan": "#389994",
		"terminal.ansiGreen": "#4da54a",
		"terminal.ansiMagenta": "#a13e80",
		"terminal.ansiRed": "#c54040",
		"terminal.ansiWhite": "#d1d1d1",
		"terminal.ansiYellow": "#d4b038",

		"terminal.ansiBrightBlack": "#5e5e5e",
		"terminal.ansiBrightBlue": "#7d90c7",
		"terminal.ansiBrightCyan": "#54c2bc",
		"terminal.ansiBrightGreen": "#72b96f",
		"terminal.ansiBrightMagenta": "#bb73a3",
		"terminal.ansiBrightRed": "#e06868",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiBrightYellow": "#dbc169",
		
		"terminal.selectionBackground": "#ffdebf1a",
		"terminalCursor.background": "#fcfcfc",
		"terminalCursor.foreground": "#c99c6a",


		// Source Tabs
        "editorGroup.border": "#141414",
        "editorGroupHeader.tabsBackground": "#2e2e30",
		
        "tab.activeBackground": "#272729",
        "tab.inactiveBackground": "#00000000",
        "tab.activeForeground": "#a9a9ad",
        "tab.border": "#141414",
		
		// Titlebar
        "titleBar.activeBackground": "#28282b",
        "titleBar.activeForeground": "#abafb6",
        "titleBar.inactiveBackground": "#28282b",
        "titleBar.inactiveForeground": "#abafb6",

		// Status Bar
        "statusBar.background": "#1d1e1f",
        "statusBar.foreground": "#abaeb9",
        "statusBarItem.hoverBackground": "#3e3e3f",
        "statusBar.noFolderBackground": "#1d1e1f",
        "statusBar.debuggingForeground": "#cc7e3e",

		// Minimaps
		"editorOverviewRuler.border": "#00000000",
		"minimap.findMatchHighlight": "#fff",


		// dropdowns
        "dropdown.background": "#333335",
        "dropdown.border": "#000000",

		// Buttons
        "button.background": "#bb8445",
        "button.hoverBackground": "#997449",
        "button.foreground": "#FFFFFF",

        "scrollbarSlider.background": "#46495080",
        "scrollbarSlider.activeBackground": "#46495080",
        "scrollbarSlider.hoverBackground": "#5f636b80",

        "focusBorder": "#bb8445",
		
        "input.background": "#343436",
        "input.border": "#1d1e1f",

		"widget.shadow": "#0000006b",

		// Notifications

		"notificationCenter.border": "#141414",
		"notificationCenterHeader.foreground": "#bbbbbb",
		"notificationCenterHeader.background": "#2e2e30",

		"notificationToast.border": "#141414",
		"notifications.background": "#39393b",
		"notifications.foreground": "#bebebe",
		"notifications.border": "#141414",

		"notificationLink.foreground": "#d3d3d3",
		"notificationsErrorIcon.foreground": "#c75353",
		"notificationsWarningIcon.foreground": "#cc994c",
		"notificationsInfoIcon.foreground": "#4f83d1",


		// GIT
		"gitDecoration.addedResourceForeground": "#90ca59",
		"gitDecoration.modifiedResourceForeground": "#b99a76",
		"gitDecoration.deletedResourceForeground": "#b15050",
		"gitDecoration.renamedResourceForeground": "#4ab4c2",
		"gitDecoration.untrackedResourceForeground": "#4f7546",
		"gitDecoration.ignoredResourceForeground": "#636366",

		"gitDecoration.stageModifiedResourceForeground": "#e2ab6c",
		"gitDecoration.stageDeletedResourceForeground": "#964a4a",
		"gitDecoration.conflictingResourceForeground": "#d61e55",
		
	},
	"tokenColors": [
		{
			"name": "Default",
			"scope": [
				"source",
			],
			"settings": {
				"foreground": "#9f9fa8"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment.line.double-slash",
                "comment.block.documentation"
			],
			"settings": {
				"foreground": "#615659"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"keyword.operator.quantifier.regexp",
				"string.regexp"
			],
			"settings": {
				"foreground": "#c57158",
			}
		},
		{
			"name": "Regex Escape|or",
			"scope": [
				"constant.character.escape.backslash.regexp",
				"keyword.operator.or.regexp"
			],
			"settings": {
				"foreground": "#d4a44a",
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.quoted.single",
				"string.quoted.double",
			],
			"settings": {
				"foreground": "#b97575",
			}
		},
		{
			"name": "String Template Expression",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
			],
			"settings": {
				"foreground": "#834444",
			}
		},



		{
			"name": "Import, Export",
			"scope": [
				"keyword.control.import",
				"keyword.control.export",
                "keyword.control.default",
			],
			"settings": {
				"foreground": "#bd4559",
				"fontStyle": "italic"
			}
		},
        
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"keyword.operator.expression",
                "keyword.operator.new",
				"storage.modifier",

				"variable.language.this",

			],
			"settings": {
				"foreground": "#c29e52",
				"fontStyle": "italic"
			}
		},







		{
			"name": "Variable Declaration",
			"scope": [
                "storage.type",
                "punctuation.definition.block.tag"
			],
			"settings": {
				"foreground": "#bd4559",
			}
		},
		{
			"name": "Variable",
			"scope": [
                "variable.other.readwrite",
                "variable.other.readwrite.alias",
                "variable.other",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#bd9d81",
			}
		},
		{
			"name": "Constant",
			"scope": [
                "variable.other.constant",
			],
			"settings": {
				"foreground": "#c97b57",
			}
		},
		{
			"name": "Lang Constant, Number Constant",
			"scope": [
                "constant.language.boolean.false",
                "constant.language.boolean.true",
                "constant.language.undefined",
                "constant.language.null",
				"constant.numeric.decimal",
				"constant.language.nan"
			],
			"settings": {
				"foreground": "#c97b57",
			}
		},



		{
			"name": "Object",
			"scope": [
                "variable.other.object",
			],
			"settings": {
				"foreground": "#416799",
			}
		},
		{
			"name": "Object Peoperty",
			"scope": [
                "variable.other.property",
                "variable.other.object.property",
				"support.variable.property",
			],
			"settings": {
				"foreground": "#567ea3",
			}
		},



		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function",
			],
			"settings": {
				"foreground": "#508896",
			}
		},
		{
			"name": "Function Call",
			"scope": [
				"meta.function-call",
			],
			"settings": {
				"foreground": "#cca77d",
			}
		},
		{
			"name": "Class",
			"scope": [
				"support.class",
			],
			"settings": {
				"foreground": "#5e93c4",
			}
		},



		{
			"name": "Brace, Bracket, Block",
			"scope": [
				"punctuation.definition.block",
                "meta.brace.round",
                "meta.brace.square",
                "keyword.operator.type.annotation",
                "keyword.operator.type",
                "punctuation.definition.parameters.begin",
                "punctuation.definition.parameters.end"
			],
			"settings": {
				"foreground": "#918888",
			}
		},
		{
			"name": "Punctuation",
			"scope": [
                "punctuation.accessor",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.definition.typeparameters",
                "meta.delimiter.decimal.period",
                "storage.type.function.arrow",
			],
			"settings": {
				"foreground": "#8a7c7c",
			}
		},
		{
			"name": "Operator",
			"scope": [
                "keyword.operator",
			],
			"settings": {
				"foreground": "#975c5a",
			}
		},



		{
			"name": "Entity Type",
			"scope": [
				"entity.name.type",
                "support.type.primitive",
                "support.type.builtin",
				"entity.name.type.module",
			],
			"settings": {
				"foreground": "#55917a",
				"fontStyle": "italic"
			}
		},












		{
			"name": "HTML",
			"scope": [
				"text.html.derivative",
			],
			"settings": {
				"foreground": "#a1a4a8",
			}
		},
		{
			"name": "HTML Tag",
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#627796",
			}
		},
		{
			"name": "HTML Punctuation",
			"scope": [
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end",
			],
			"settings": {
				"foreground": "#606468",
			}
		},
		{
			"name": "HTML Attribute",
			"scope": [
				"entity.other.attribute-name",
			],
			"settings": {
				"foreground": "#8b6d83",
			}
		},
		{
			"name": "HTML Component",
			"scope": [
				"support.class.component",
			],
			"settings": {
				"foreground": "#c09f79",
			}
		},
		{
			"name": "HTML String",
			"scope": [
				"string.quoted.single.html",
				"string.quoted.double.html",
				"string.quoted.svelte",
			],
			"settings": {
				"foreground": "#b3856f",
			}
		},





		{
			"name": "CSS Selector (name, class)",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.name.tag.css",
			],
			"settings": {
				"foreground": "#c4a77c",
			}
		},
		{
			"name": "CSS ID Selector",
			"scope": [
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#5879d1",
			}
		},
		{
			"name": "CSS Attribute Selector",
			"scope": [
				"entity.other.attribute-name.attribute.css",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.attribute.sass",
			],
			"settings": {
				"foreground": "#a88181",
			}
		},
		{
			"name": "CSS Pseudo-element",
			"scope": [
				"support.function.misc.css",
				"support.function.misc.scss",
				"support.function.misc.sass",
			],
			"settings": {
				"foreground": "#578ca1",
			}
		},
		{
			"name": "CSS Property Name",
			"scope": [
				"support.type.property-name.css",
			],
			"settings": {
				"foreground": "#6f747e",
			}
		},
		{
			"name": "CSS Property Value",
			"scope": [
				"meta.property-value",
			],
			"settings": {
				"foreground": "#afafaf",
			}
		},
		{
			"name": "CSS String",
			"scope": [
				"string.quoted.single.css",
				"string.quoted.single.scss",
				"string.quoted.single.sass",
				
			],
			"settings": {
				"foreground": "#b3856f",
			}
		},
		{
			"name": "CSS Brackets, Other",
			"scope": [
				"punctuation.section.property-list.begin.bracket.curly",
				"punctuation.section.property-list.end.bracket.curly",
				"punctuation.section.function",
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end",
				"punctuation.definition.block.begin",
				"punctuation.definition.block.end",
			],
			"settings": {
				"foreground": "#9398a0",
			}
		},
		{
			"name": "CSS Variable",
			"scope": [
				"variable.css",
				"variable.scss",
				"variable.sass",
			],
			"settings": {
				"foreground": "#a15773",
			}
		},
		{
			"name": "CSS Function",
			"scope": [
				"support.function.misc.css",
				"support.function.misc.scss",
				"support.function.misc.sass",
			],
			"settings": {
				"foreground": "#578ca1",
			}
		},
		{
			"name": "CSS/SCSS/SASS Tag Reference",
			"scope": [
				"entity.name.tag.reference",
			],
			"settings": {
				"foreground": "#a15773",
			}
		},





		// JSON
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "JSON key",
			"scope": [
				"support.type.property-name.json",
			],
			"settings": {
				"foreground": "#6f747e"
			}
		},
		{
			"name": "JSON Const",
			"scope": [
				"constant.language.json",
			],
			"settings": {
				"foreground": "#4863ad"
			}
		},
		{
			"name": "JSON string",
			"scope": [
				"string.quoted.double.json",
			],
			"settings": {
				"foreground": "#aa5e5e"
			}
		},


		// Other 
		{
			"name": "Property Name",
			"scope": [
				"support.type.property-name",
			],
			"settings": {
				"foreground": "#6f747e",
			}
		},
	]
}
