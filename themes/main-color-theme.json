		"editor.selectionBackground": "#3f3c41",
		
        "editorSuggestWidget.background": "#353135",
		"editorSuggestWidget.foreground": "#837d83",
        "editorSuggestWidget.border": "#1a1a1a",
		"editorSuggestWidget.highlightForeground": "#B56576",
		"editorSuggestWidget.focusHighlightForeground": "#E56B6F",
        "editorSuggestWidget.selectedBackground": "#413c41",
		"editorSuggestWidget.selectedForeground": "#c0b9c0",

		"editorBracketMatch.background": "#00000000",
		"editorBracketMatch.border": "#E56B6F",
		"editorBracketHighlight.unexpectedBracket.foreground": "#db3737",
		"editorBracketPairGuide.activeBackground1": "#9276a5",
		"editorBracketPairGuide.activeBackground2": "#B56576",
		"editorBracketPairGuide.activeBackground3": "#EAAC8B",
		"editorBracketPairGuide.activeBackground4": "#9276a5",
		"editorBracketPairGuide.activeBackground5": "#B56576",
		"editorBracketPairGuide.activeBackground6": "#EAAC8B",
		"editorBracketPairGuide.background1": "#9276a5",
		"editorBracketPairGuide.background2": "#B56576",
		"editorBracketPairGuide.background3": "#EAAC8B",
		"editorBracketPairGuide.background4": "#9276a5",
		"editorBracketPairGuide.background5": "#B56576",
		"editorBracketPairGuide.background6": "#EAAC8B",

		"editorBracketHighlight.foreground1": "#9276a5",
		"editorBracketHighlight.foreground2": "#B56576",
		"editorBracketHighlight.foreground3": "#EAAC8B",
		"editorBracketHighlight.foreground4": "#9276a5",
		"editorBracketHighlight.foreground5": "#B56576",
		"editorBracketHighlight.foreground6": "#EAAC8B",
		
		// Titlebar
		"titleBar.activeBackground": "#221f22",
		"titleBar.inactiveBackground": "#221f22",
		"titleBar.activeForeground": "#6d6d6d",
		"titleBar.inactiveForeground": "#494949",
		"titleBar.border": "#0000004f",

		// Statusbar
		"statusBar.background": "#221f22",
		"statusBar.border": "#000000",

		// Minimap 
		"editorOverviewRuler.border": "#ffffff00",
		"minimap.findMatchHighlight": "#E56B6F",

		// Widgets
		"widget.shadow": "#0000004b",

		// Git 
		"editorGutter.addedBackground": "#70a064",
		"editorGutter.deletedBackground": "#E56B6F",
		"editorGutter.modifiedBackground": "#6D597A",
		"editorGutter.foldingControlForeground": "#5a565a",

		"gitDecoration.addedResourceForeground": "#70a064",
		"gitDecoration.modifiedResourceForeground": "#c09985",
		"gitDecoration.deletedResourceForeground": "#B56576",
		"gitDecoration.renamedResourceForeground": "#5a5b8f",
		"gitDecoration.untrackedResourceForeground": "#7c678a",
		"gitDecoration.ignoredResourceForeground": "#5a565a",

		"gitDecoration.stageModifiedResourceForeground": "#d4a084",
		"gitDecoration.stageDeletedResourceForeground": "#a03b51",
		"gitDecoration.conflictingResourceForeground": "#ccae29",

		// Dropdowns
		"dropdown.background": "#221f22",
		"dropdown.border": "#3a383a",
		"dropdown.foreground": "#949494",

		// Panels
		"panel.background": "#2d2a2e",
		"panel.border": "#3a383a",

		// Terminal

		"terminal.foreground": "#cacaca",
		
		"terminal.ansiBlack": "#1d1d1d",
		"terminal.ansiRed": "#d80000",
		"terminal.ansiBlue": "#3758b3",
		"terminal.ansiCyan": "#00818a",
		"terminal.ansiGreen": "#4ca55f",
		"terminal.ansiMagenta": "#bb579a",
		"terminal.ansiWhite": "#cacaca",
		"terminal.ansiYellow": "#f0ad48",
		
		"terminal.ansiBrightBlack": "#383838",
		"terminal.ansiBrightRed": "#fc8787",
		"terminal.ansiBrightBlue": "#677fc0",
		"terminal.ansiBrightCyan": "#6ec6cc",
		"terminal.ansiBrightGreen": "#81c790",
		"terminal.ansiBrightMagenta": "#e090c6",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiBrightYellow": "#ffcd83",

		// Other
		"input.background": "#221f22",

	},      
	"tokenColors": [
		{
			"name": "Default",
			"scope": [
				"source",
			],
			"settings": {
				"foreground": "#9f9fa8"
			}
		},
		{
			"name": "Parahgraph",
			"scope": [
				"meta.paragraph",
			],
			"settings": {
				"foreground": "#8b8b92"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.quoted.single",
				"string.quoted.double",
				"string.unquoted",
				"fenced_code.block.language"
			],
			"settings": {
				"foreground": "#3f946a",
			}
		},
		{
			"name": "Link",
			"scope": [
				"meta.link",
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#466fac",
			}
		},
		{
			"name": "Heading",
			"scope": [
				"heading",
			],
			"settings": {
				"foreground": "#bd4559"
			}
		},
		{
			"name": "Reference",
			"scope": [
				"meta.link.reference",
			],
			"settings": {
				"foreground": "#bd4559"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment.line",
                "comment.block.documentation",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#615659"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"keyword.operator.quantifier.regexp",
				"string.regexp"
			],
			"settings": {
				"foreground": "#c57158",
			}
		},
		{
			"name": "Regex Escape|or",
			"scope": [
				"constant.character.escape.backslash.regexp",
				"keyword.operator.or.regexp"
			],
			"settings": {
				"foreground": "#d4a44a",
			}
		},
		{
			"name": "String Template Expression",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
			],
			"settings": {
				"foreground": "#1f9b8b",
			}
		},



		{
			"name": "Import, Export, Default, From",
			"scope": [
				"keyword.control.import",
				"keyword.control.export",
                "keyword.control.default",
                "keyword.control.from",
			],
			"settings": {
				"foreground": "#bd4559",
				"fontStyle": "italic"
			}
		},
        
		{
			"name": "Keyword",
			"scope": [
				"keyword.control",
				"keyword.operator.expression",
                "keyword.operator.new",
				"storage.modifier",

				"variable.language.this",

			],
			"settings": {
				"foreground": "#da945b"
			}
		},







		{
			"name": "Variable Declaration",
			"scope": [
                "storage.type",
                "punctuation.definition.block.tag"
			],
			"settings": {
				"foreground": "#aa3f51",
			}
		},
		{
			"name": "Variable",
			"scope": [
                "variable.other.readwrite",
                "variable.other.readwrite.alias",
                "variable.other",
			],
			"settings": {
				"foreground": "#918d8b",
			}
		},
		{
			"name": "Constant",
			"scope": [
                "variable.other.constant",
				"support.constant"
			],
			"settings": {
				"foreground": "#be7d64",
			}
		},
		{
			"name": "Lang Constant, Number Constant",
			"scope": [
                "constant.language.boolean.false",
                "constant.language.boolean.true",
                "constant.language.undefined",
                "constant.language.null",
				"constant.numeric.decimal",
				"constant.language.nan"
			],
			"settings": {
				"foreground": "#be7d64",
			}
		},



		{
			"name": "Object",
			"scope": [
                "variable.other.object",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#968d87",
			}
		},
		{
			"name": "Object Peoperty",
			"scope": [
                "variable.other.property",
                "variable.other.object.property",
				"variable.object.property",
				"support.variable.property",
				"meta.object-literal.key",
				"meta.object.member",
				"meta.objectliteral"
			],
			"settings": {
				"foreground": "#a35a6c",
			}
		},



		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function",
			],
			"settings": {
				"foreground": "#637eb8",
			}
		},
		{
			"name": "Function Call",
			"scope": [
				"meta.function-call",
			],
			"settings": {
				"foreground": "#6d5ac2",
			}
		},
		{
			"name": "Class",
			"scope": [
				"support.class",
			],
			"settings": {
				"foreground": "#6d5ac2",
			}
		},
		{
			"name": "Parameter Closures",
			"scope": [
				"punctuation.definition.typeparameters.begin",
				"punctuation.definition.typeparameters.end",
			],
			"settings": {
				"foreground": "#445c72",
			}
		},




		{
			"name": "Brace, Bracket, Block",
			"scope": [
				"punctuation.definition.block",
                "meta.brace.round",
                "meta.brace.square",
                "keyword.operator.type.annotation",
                "keyword.operator.type",
                "punctuation.definition.parameters.begin",
                "punctuation.definition.parameters.end",
				"meta.embedded.block"
			],
			"settings": {
				"foreground": "#918888",
			}
		},
		{
			"name": "Punctuation",
			"scope": [
                "punctuation.accessor",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.definition.typeparameters",
                "meta.delimiter.decimal.period",
                "storage.type.function.arrow",
			],
			"settings": {
				"foreground": "#677388",
			}
		},
		{
			"name": "Operator",
			"scope": [
                "keyword.operator",
			],
			"settings": {
				"foreground": "#975c5a",
			}
		},



		{
			"name": "Entity Type",
			"scope": [
				"entity.name.type",
                "support.type.primitive",
                "support.type.builtin",
				"entity.name.type.module",
				"keyword.operator.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#445c72",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Entity Type Operator",
			"scope": [
				"keyword.operator.type",
			],
			"settings": {
				"foreground": "#677388"
			}
		},



		


		{
			"name": "HTML",
			"scope": [
				"text.html.derivative",
			],
			"settings": {
				"foreground": "#a1a4a8",
			}
		},
		{
			"name": "HTML Tag",
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#b15060",
			}
		},
		{
			"name": "HTML Punctuation",
			"scope": [
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end",
			],
			"settings": {
				"foreground": "#834650",
			}
		},
		{
			"name": "Attribute (including HTML)",
			"scope": [
				"entity.other.attribute-name",
			],
			"settings": {
				"foreground": "#b3856f",
			}
		},
		{
			"name": "HTML Component",
			"scope": [
				"support.class.component",
			],
			"settings": {
				"foreground": "#c09f79",
			}
		},
		{
			"name": "HTML String",
			"scope": [
				"string.quoted.single.html",
				"string.quoted.double.html",
				"string.quoted.svelte",
			],
			"settings": {
				"foreground": "#606e94",
			}
		},
		{
			"name": "HTML scoped tag",
			"scope": [
				"meta.scope.tag",
			],
			"settings": {
				"foreground": "#916857",
			}
		},





		{
			"name": "CSS TAG",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.name.tag.css",
			],
			"settings": {
				"foreground": "#b15060",
			}
		},
		{
			"name": "CSS CLASS",
			"scope": [
				"entity.other.attribute-name.class.css",
			],
			"settings": {
				"foreground": "#c48b5d",
			}
		},
		{
			"name": "CSS ID Selector",
			"scope": [
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#6a80bd",
			}
		},
		{
			"name": "CSS Attribute Selector",
			"scope": [
				"source.css entity.other.attribute-name",
			],
			"settings": {
				"foreground": "#3fa2af",
			}
		},
		{
			"name": "CSS Attribute String",
			"scope": [
				"source.css meta.attribute-selector keyword.operator.pattern",
				"source.css meta.attribute-selector",
				"source.css meta.attribute-selector punctuation.definition.string.begin",
				"source.css meta.attribute-selector punctuation.definition.string.end",

				"source.css meta.attribute-selector string.quoted.double",
				"source.css meta.attribute-selector string.quoted.single",
				"source.css meta.attribute-selector string.unquoted",
			],
			"settings": {
				"foreground": "#407b83",
			}
		},
		{
			"name": "CSS Attribute operator",
			"scope": [
				"source.css keyword.operator.pattern",
			],
			"settings": {
				"foreground": "#9398a0",
			}
		},
		{
			"name": "CSS Pseudo-element",
			"scope": [
				"source.css entity.other.attribute-name.pseudo-element",
				"entity.other.attribute-name.pseudo-class.css"
			],
			"settings": {
				"foreground": "#7f67aa",
			}
		},
		{
			"name": "CSS Property Name",
			"scope": [
				"support.type.property-name.css",
			],
			"settings": {
				"foreground": "#7a7d85",
			}
		},
		{
			"name": "CSS Property Value",
			"scope": [
				"meta.property-value",
			],
			"settings": {
				"foreground": "#979ba3",
			}
		},
		{
			"name": "CSS String",
			"scope": [
				"string.quoted.single.css",
				"string.quoted.single.scss",
				"string.quoted.single.sass",
				"string.quoted.double.css",
				"string.quoted.double.scss",
				"string.quoted.double.sass",
				"string.unquoted.css",
				"string.unquoted.scss",
				"string.unquoted.sass",
				
			],
			"settings": {
				"foreground": "#b38653",
			}
		},
		{
			"name": "CSS Brackets, Other",
			"scope": [
				"punctuation.section",
				"punctuation.definition",
				"punctuation.terminator",
				"punctuation.separator",
				"source.css meta.attribute-selector punctuation.definition.attribute-selector.end.bracket.square",
				"source.css meta.attribute-selector punctuation.definition.attribute-selector.begin.bracket.square",
			],
			"settings": {
				"foreground": "#5e6166",
			}
		},
		{
			"name": "CSS Variable",
			"scope": [
				"variable.css",
				"variable.scss",
				"variable.sass",
			],
			"settings": {
				"foreground": "#3f946a",
			}
		},
		{
			"name": "CSS Function",
			"scope": [
				"source.css support.function"
			],
			"settings": {
				"foreground": "#3c8a86",
			}
		},
		{
			"name": "CSS Unit",
			"scope": [
				"source.css keyword.other.unit"
			],
			"settings": {
				"foreground": "#834949",
			}
		},
		{
			"name": "CSS Number",
			"scope": [
				"source.css constant.numeric"
			],
			"settings": {
				"foreground": "#aa5252",
			}
		},
		{
			"name": "CSS Keyword",
			"scope": [
				"source.css keyword.control"
			],
			"settings": {
				"foreground": "#97566a",
			}
		},
		{
			"name": "CSS Hex",
			"scope": [
				"source.css constant.other.color.rgb-value.hex"
			],
			"settings": {
				"foreground": "#388b87",
			}
		},
		{
			"name": "CSS Important/keywords",
			"scope": [
				"source.css keyword.other"
			],
			"settings": {
				"foreground": "#36a3be",
			}
		},





		// JSON
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "JSON key",
			"scope": [
				"support.type.property-name.json",
			],
			"settings": {
				"foreground": "#964e5a"
			}
		},
		{
			"name": "JSON Const",
			"scope": [
				"constant.language.json",
			],
			"settings": {
				"foreground": "#4863ad"
			}
		},
		{
			"name": "JSON string",
			"scope": [
				"string.quoted.double.json",
			],
			"settings": {
				"foreground": "#b68e5a"
			}
		},


		// Other 
		{
			"name": "Property Name",
			"scope": [
				"support.type.property-name",
			],
			"settings": {
				"foreground": "#6f747e",
			}
		},
	]
}
