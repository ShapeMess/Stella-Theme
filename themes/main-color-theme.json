	"tokenColors": [
		{
			"name": "Default",
			"scope": [
				"source",
			],
			"settings": {
				"foreground": "#9f9fa8"
			}
		},
		{
			"name": "Parahgraph",
			"scope": [
				"meta.paragraph",
			],
			"settings": {
				"foreground": "#8b8b92"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.quoted.single",
				"string.quoted.double",
				"string.unquoted",
				"fenced_code.block.language"
			],
			"settings": {
				"foreground": "#cc9270",
			}
		},
		{
			"name": "Link",
			"scope": [
				"meta.link",
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#466fac",
			}
		},
		{
			"name": "Heading",
			"scope": [
				"heading",
			],
			"settings": {
				"foreground": "#bd4559"
			}
		},
		{
			"name": "Reference",
			"scope": [
				"meta.link.reference",
			],
			"settings": {
				"foreground": "#bd4559"
			}
		},
		{
